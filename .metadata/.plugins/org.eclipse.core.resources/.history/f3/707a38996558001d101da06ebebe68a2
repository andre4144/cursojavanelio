package entities;

public class Rectangule {
	public double a;
	public double b;

	public double area() {
		return a * b;
	}

	public double perimeter() {
		return 2 * (a + b);
	}

	public double diagonal() {
		return Math.sqrt(a * a + b * b);
	}

	public String toString () {
		return "AREA = "+ String.format("%2.f", area()) + "PERIMETER = "+ String.format("%2.f", perimeter())  + "DIAGONAL = " + String.format("%2.f", diagonal())  ; 
				
	}

}
